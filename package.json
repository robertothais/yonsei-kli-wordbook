    "shuffle-array": "^1.0.1"
  },
  "devDependencies": {
    "@types/mousetrap": "^1.6.0",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/shuffle-array": "^0.0.28",
    "node-sass": "^4.9.2",
